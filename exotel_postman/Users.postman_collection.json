          ],
          "query": [
            {
              "key": "fields",
              "value": "devices,active_call",
              "description": "string; to add more fields in the GET response of the user objects.\n\nSupported fields\n\ndevices : If this is set, response will contain the block that will list all the devices under the user\nactive_call : If this is set, response will indicate whether the users are on call or not\nlast_login : If this is set, response will indicate the last login status of the user\nExample:\n/users?fields=devices,active_call,last_login"
            },
            {
              "key": "devices.contact_uri",
              "value": "07905690278",
              "description": "string; Your user's device contact URI i.e. phone number in E.164 format or SIP address if VOIP is enabled. The values should be URL encoded, like + should be replaced with %2B.\n\nExample:\n/users?devices.contact_uri=+9199512131xx \n/users?devices.contact_uri=+9199512112xx,+9199512113xx\n/users?devices.contact_uri=sip:1-999-123-4567@voip.exotel.com"
            },
            {
              "key": "email",
              "value": "email_id_1,email_id_2",
              "description": "string; Your user's email address as added on Exotel. The values should be URL encoded.\n\nExample:\n/users?email=<email-id>\n/users?email=<email-id-1>,<email-id-2>",
              "type": "text",
              "disabled": true
            }
